action (RollDice) {
  collect{
    input (numDice) {
      type (NumDiceConcept)
      min (Required)
      max (One)
    }
    input (gameName) {
      type (GameName)
      min (Required)
      max (One)
    }
    input (playerNum) {
      type (PlayerNum)
      min (Required)
      max (One)
    }
    input (numSides) {
      type (NumSidesConcept)
      min (Required)
      max (One)
    }
  } 
  output (RollResultConcept)
  type (Calculation)
}